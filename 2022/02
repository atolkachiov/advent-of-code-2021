        "file = open(\"02.txt\", \"r\")\n",
        "lines = file.readlines()\n",
        "file.close()\n",
        "\n",
        "# Rock, Paper, Scissors\n",
        "dict = {\n",
        " \"X\": {\"A\": 3, \"B\": 0, \"C\": 6},\n",
        " \"Y\": {\"A\": 6, \"B\": 3, \"C\": 0},\n",
        " \"Z\": {\"A\": 0, \"B\": 6, \"C\": 3},\n",
        "}\n",
        "\n",
        "scores = {\n",
        "    \"X\": 1, \"Y\": 2, \"Z\": 3\n",
        "}\n",
        "\n",
        "score = 0\n",
        "for l in lines:\n",
        "  st = l.strip()\n",
        "  a = st[0]\n",
        "  b = st[2]\n",
        "  score = score + dict[b][a] + scores[b]\n",
        "\n",
        "score\n"
