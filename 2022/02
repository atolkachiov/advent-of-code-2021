        "# Part2 \n",
        "\n",
        "# Rock, Paper, Scissors\n",
        "# X means you need to lose, Y means you need to end the round in a draw, and Z means you need to win\n",
        "dict = {\n",
        " \"X\": {\"A\": 3, \"B\": 1, \"C\": 2},\n",
        " \"Y\": {\"A\": 1, \"B\": 2, \"C\": 3},\n",
        " \"Z\": {\"A\": 2, \"B\": 3, \"C\": 1},\n",
        "}\n",
        "\n",
        "scores = {\n",
        "    \"X\": 0, \"Y\": 3, \"Z\": 6,\n",
        "}\n",
        "\n",
        "score = 0\n",
        "for l in lines:\n",
        "  st = l.strip()\n",
        "  a = st[0]\n",
        "  b = st[2]\n",
        "  score = score + dict[b][a] + scores[b]\n",
        "\n",
        "score"
